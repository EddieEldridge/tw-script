{
  "name": "tw-script",
  "displayName": "tw-script",
  "description": "Language extension for working with Medieval 2: Total War text files",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/EddieEldridge/tw-script",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:tw-script"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "tw-script",
        "aliases": [
          "tw-script"
        ],
        "extensions": [
          "campaign_script.txt",
          "export_descr_buildings.txt",
          "export_descr_ancillaries.txt",
          "export_descr_character_traits.txt",
          "export_buildings.txt",
          "export_ancillaries.txt",
          "export_traits.txt",
          "export_vnvs.txt",
          "descr_strat.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tw-script",
        "scopeName": "source.med",
        "path": "./syntaxes/tw-script.tmLanguage.json"
      }
    ]
  },
  "__metadata": {
    "isApplicationScoped": false,
    "installedTimestamp": 1717509272971,
    "pinned": false,
    "source": "vsix"
  }
}